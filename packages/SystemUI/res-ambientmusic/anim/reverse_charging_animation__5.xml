        android:valueFrom="0.3"
        android:valueTo="0.3"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="101"
        android:interpolator="@anim/reverse_charging_animation__5__1"
        android:propertyName="scaleY"
        android:startOffset="0"
        android:valueFrom="0.3"
        android:valueTo="0.3"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="316"
        android:interpolator="@anim/reverse_charging_animation__5__2"
        android:propertyName="scaleX"
        android:startOffset="101"
        android:valueFrom="0.3"
        android:valueTo="1"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="316"
        android:interpolator="@anim/reverse_charging_animation__5__3"
        android:propertyName="scaleY"
        android:startOffset="101"
        android:valueFrom="0.3"
        android:valueTo="1"
        android:valueType="floatType" />

</set>
