<?xml version="1.0" encoding="utf-8"?>
<set
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:ordering="together">

    <objectAnimator
        android:duration="17"
        android:interpolator="@anim/reverse_charging_animation__2__0"
        android:propertyName="scaleX"
        android:startOffset="0"
        android:valueFrom="0.5"
        android:valueTo="0.5"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="17"
        android:interpolator="@anim/reverse_charging_animation__2__1"
        android:propertyName="scaleY"
        android:startOffset="0"
        android:valueFrom="0.5"
        android:valueTo="0.5"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="133"
        android:interpolator="@anim/reverse_charging_animation__2__2"
        android:propertyName="scaleX"
        android:startOffset="17"
        android:valueFrom="0.5"
        android:valueTo="1.04"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="133"
        android:interpolator="@anim/reverse_charging_animation__2__3"
        android:propertyName="scaleY"
        android:startOffset="17"
        android:valueFrom="0.5"
        android:valueTo="1.04"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="267"
        android:interpolator="@anim/reverse_charging_animation__2__4"
        android:propertyName="scaleX"
        android:startOffset="150"
        android:valueFrom="1.04"
        android:valueTo="1"
        android:valueType="floatType" />

    <objectAnimator
        android:duration="267"
        android:interpolator="@anim/reverse_charging_animation__2__5"
        android:propertyName="scaleY"
        android:startOffset="150"
        android:valueFrom="1.04"
        android:valueTo="1"
        android:valueType="floatType" />

</set>
